@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>

<a asp-action="Create" asp-controller="Role">Create Role</a>
<table class="table-bordered table table-dark" >
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
            <tr>
                <td>@role.Id</td>
                <td>@role.Name</td>
            </tr>
        }
    </tbody>
</table>